{
  "name": "Github XP",
  "version": "5.03",
  "description": "Github <3 Microsoft!",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "xp.css"
      ]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
